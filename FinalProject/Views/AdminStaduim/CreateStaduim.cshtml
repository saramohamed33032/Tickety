@model FinalProject.Models.TicketyModel.Staduim
@{
    Layout = null;
    <link href="~/css/style.css" rel="stylesheet" />
    var message = ViewBag.error;
}
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <h4>Staduim</h4>
    <hr />
    @Html.ValidationSummary(true, "", new { @class = "text-danger" })
    @Html.LabelFor(model => model.staduimName, htmlAttributes: new { @class = "stadiumlbl" })
    @Html.EditorFor(model => model.staduimName, new { htmlAttributes = new { @class = "stdtxt", required = "required" } })
    @*@Html.ValidationMessageFor(model => model.staduimName, "", new { @class = "text-danger" })*@
    @Html.LabelFor(model => model.city, htmlAttributes: new { @class = "stadiumlbl " })
    @Html.EditorFor(model => model.city, new { htmlAttributes = new { @class = "stdtxt" , required = "required" } })
    @*@Html.ValidationMessageFor(model => model.city, "", new { @class = "text-danger" })*@
    @Html.LabelFor(model => model.staduimLocation, htmlAttributes: new { @class = "stadiumlbl " })
    @Html.EditorFor(model => model.staduimLocation, new { htmlAttributes = new { @class = "stdtxt" , required = "required" } })
    @*@Html.ValidationMessageFor(model => model.staduimLocation, "", new { @class = "text-danger" })*@



    <div class="form-group ">
        <div class="col-md-offset-2 col-md-10 text-right">
            <input type="submit" value="Create" class="btn btn-default btn-lg btnAddSta" />
        </div>
    </div>
   
}


